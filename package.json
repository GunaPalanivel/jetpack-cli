{
  "name": "jetpack-cli",
  "version": "0.4.0",
  "description": "Zero-config developer onboarding orchestrator using GitHub Copilot CLI",
  "main": "bin/jetpack.js",
  "bin": {
    "jetpack": "./bin/jetpack.js"
  },
  "scripts": {
    "start": "node bin/jetpack.js",
    "dev": "node bin/jetpack.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:rollback": "jest --selectProjects rollback",
    "test:all": "npm test"
  },
  "keywords": [
    "cli",
    "onboarding",
    "developer-tools",
    "copilot",
    "automation",
    "devops"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "blessed": "^0.1.81",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0"
  }
}